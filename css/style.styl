//stylus -u nib -w style.styl --compress
//stylus-spriting icons/*.png --image <file>
//@media screen and (max-width: 1200px)
//@media screen and (max-width: 767px)
//transition all 0.3s ease

@import 'nib'
@import 'fonts'
@import 'variables'
@import 'sprite'
@import 'extends'
@import 'globals'
@import 'chosen'

.layout-header
    height 134px
    @media screen and (max-width: 767px)
        height 80px
        border-bottom 1px solid white
        transition all 0.2s ease

.layout-content
    margin-bottom 20px

.header-top
    box-sizing border-box
    height 24px
    border-bottom 1px solid grayBorder
    box-shadow 0 1px 0 white
    z-index 2
    position relative
    @media screen and (max-width: 767px)
        height 26px
    &-content
        height 23px
        display flex
        justify-content space-between
        &-box
            display flex
            position relative
            @media screen and (max-width: 767px)
                display none
            &.left
                flex 0 0 245px
                @media screen and (max-width: 1200px)
                    flex 0 0 190px
        &_time
            display flex
            font-size 12px
            color grayColor
            align-items center
            margin-right 70px
            flex 0 0 auto
            @media screen and (max-width: 1200px)
                margin-right 20px
            &:before
                content ''
                @extend .sprite-icon-top-time
                margin-right 7px
        &_langs
            position absolute
            box-sizing border-box
            width 50px
            cursor pointer
            font-size 12px
            color grayColor
            right 0
            top 1px
            animation-duration 0.5s
            &.active
                background white
                box-shadow 0 0 0 1px grayBorder, 3px 3px 7px rgba(black, 0.35)
            &-current
                display flex
                height 22px
                align-items center
                padding-left 10px
                user-select none
                .header-top-content_langs.active &
                    border-bottom 1px solid grayBorder
                &:after
                    content ''
                    width 0
                    height 0
                    border-style solid
                    border-width 3px 3.5px 0 3.5px
                    border-color #000333 transparent transparent transparent
                    margin-left 7px
            &-drop
                flex-direction column
                display none
                .header-top-content_langs.active &
                    display flex
                &-link
                    flex 0 0 22px;
                    text-decoration none
                    color grayColor
                    display flex
                    align-items center
                    padding-left 10px
                    &:not(:last-child)
                        border-bottom 1px solid grayBorder
        &_login-reg
            position relative
            &-button
                user-select none
                cursor pointer
                display flex
                height 23px
                align-items center
                font-size 12px
                color grayColor
                &:before
                    content ''
                    @extend .sprite-icon-login-reg
                    margin-right 8px
                    .header-top-content_login-reg.active &
                        @extend .sprite-icon-login-reg-active
            &-popup
                position absolute
                right 0
                top 31px
                z-index 2
                background white
                border 1px solid grayBorder
                @extend .popup-shadow
                width 480px
                box-sizing border-box
                padding 15px 10px 10px
                animation-duration 0.3s
                display none
                .header-top-content_login-reg.active &
                    display block
                &:before,
                &:after
                    content ''
                    position absolute
                    width 0
                    height 0
                    border-style solid
                &:before
                    border-width 0 8.5px 9px 8.5px
                    border-color transparent transparent grayBorder transparent
                    top -9px
                    right 105px
                &:after
                    border-width 0 7.5px 8px 7.5px
                    border-color transparent transparent white transparent
                    top -8px
                    right 106px   
                &_title
                    color black
                    font-size 20px
                    margin-bottom 8px
                &-content
                    &-form
                        flex 0 0 220px
                        font-size 12px
                        &_text
                            margin-bottom 10px
                            &:first-child
                                margin-bottom 16px
                        &-not-login
                            margin-bottom 10px
                            &_link
                                color placeholderColor
                        &-save
                            margin-bottom 14px
                            &_check
                                display none
                            &_label
                                cursor pointer
                                user-select none
                                color placeholderColor
                                &-icon
                                    flex 0 0 16px
                                    height 16px
                                    border 1px solid grayBorder
                                    border-radius 2px
                                    margin-right 5px
                                    background white
                                    &-check
                                        opacity 0
                                        visibility hidden
                                        transition all 0.2s ease
                                        .header-top-content_login-reg-popup-content-form-save_check:checked + label &
                                            opacity 1
                                            visibility visible
                        &_submit
                            width 100%
                            height 36px                  
                    &-links
                        box-sizing border-box
                        flex 0 0 220px
                        height 198px
                        &_title
                            color black
                            margin-bottom 8px
                        &_link
                            flex 0 0 30px
                            text-decoration none
                            font-size 12px
                            color white
                            padding-left 14px
                            &.fb
                                background #3a5997
                                margin-bottom 16px
                                i
                                    margin-right 19px
                            &.vk
                                background #395e88
                                i
                                    margin-right 15px
                        &_reg
                            color grayColor
                            margin-bottom 10px
        &-tell-small
            @media screen and (min-width: 767px)
                display none
        &-call-button-small
            @media screen and (min-width: 767px)
                display none
            

.header-middle
    height 70px
    position relative
    @media screen and (max-width: 767px)
        height 54px
        border-bottom 1px solid grayBorder
        box-sizing border-box
    &:before
        content ''
        position absolute
        left 0
        top 17px
        width 50%
        height 36px
        display block
        background yellowColor
        @media screen and (max-width: 767px)
            display none
    &-content
        display flex
        height 100%
        justify-content space-between
        align-items center
        position relative
        background bodyBg
        @media screen and (max-width: 767px)
            justify-content space-between
        &:before
            content ''
            position absolute
            left 0
            top 17px
            width 178px
            height 36px
            display block
            background yellowColor
            @media screen and (max-width: 767px)
                display none
    &_link
        position relative
        flex 0 0 141px
        @media screen and (max-width: 767px)
            position absolute
            left 50%
            top 50%
            width 140px
            transform translate(-50%, -50%)
            display flex
            justify-content center
            align-items center
            &:before
                content ''
                position absolute
                width 100%
                height 26px
                left 0
                top 5px
                background yellowColor
        img
            display block
            @media screen and (max-width: 767px)
                width 95px
                position relative
    &_search
        position relative
        padding-left 140px
        margin-left 100px
        @media screen and (max-width: 1200px)
            margin-left 48px
            padding-left 0
        @media screen and (max-width: 767px)
            display none
        &-cats
            position absolute
            left 0
            top 0
            font-size 12px
            width 140px
            color grayColor
            z-index 2
            transition all 0.2s ease
            @media screen and (max-width: 1200px)
                display none
            &.active
                box-shadow 3px 3px 7px rgba(black, 0.35)
            &-button
                user-select none
                cursor pointer
                display flex
                box-sizing border-box
                height 36px
                border 2px solid yellowColor
                align-items center
                justify-content space-between
                padding 0 10px 0 18px
                background white
                &:after
                    content ''
                    display block
                    width 0
                    height 0
                    border-style solid
                    border-width 4px 4px 0 4px
                    border-color #333333 transparent transparent transparent
                    .header-middle_search-cats.active &
                        border-width 0 4px 4px 4px
                        border-color transparent transparent #333333 transparent
            &-dropdown
                background white
                border 1px solid grayBorder
                border-top none
                display none
                animation-duration 0.2s
                .header-middle_search-cats.active &
                    display block
                &_item
                    display flex
                    height 34px
                    align-items center
                    padding-left 17px
                    cursor pointer
                    &:not(:last-child)
                        border-bottom 1px solid grayBorder
        &-form
            display flex
            &_field
                width 265px
                height 36px
                border-left none
                border-right none
                @media screen and (max-width: 1200px)
                    border-left 1px solid grayBorder
                    width 170px
            &_button
                cursor pointer
                border none
                flex 0 0 36px
                height 36px
                @extend .sprite-icon-search-but-active
                &:hover
                    @extend .sprite-icon-search-but
    &_tels
        display flex
        align-items center
        position relative
        @media screen and (max-width: 767px)
            display none
        &-icon
            @extend .sprite-icon-top-tel
            position relative
            top 3px
            flex 0 0 27px
        &-box
            margin 0 5px 0 5px
            line-height 0.9
        &-num
            color grayColor
            font-size 16px
            font-family fontBold
            cursor pointer
            white-space nowrap
            @media screen and (max-width: 767px)
                font-size 14px
        &-link
            font-size 12px
            color placeholderColor
            &:hover
                text-decoration underline
                color redColor
        &-arrow
            flex 0 0 20px
            height 20px
            cursor pointer
            position relative
            top 2px
            &:after
                content ''
                width 0
                height 0
                border-style solid
                border-width 4px 4px 0 4px
                border-color placeholderColor transparent transparent transparent 
                transition transform 0.3s ease
                .header-middle_tels.active &
                    transform rotate(180deg)   
        &-dropdown
            position absolute
            box-sizing border-box
            width 182px
            background white 
            left 0
            top 32px
            z-index 2
            border 1px solid grayBorder
            box-shadow 3px 3px 7px rgba(black, 0.35)
            padding 10px
            color grayColor
            font-size 16px
            font-family fontBold
            animation-duration 0.3s
            display none
            .header-middle_tels.active &
                display block
            &-num
                display flex
                align-items center
                &:not(:last-child)
                    margin-bottom 13px
                &:before
                    content ''
                    margin-right 10px
                &.num-1
                    &:before
                        @extend .sprite-icon-tel-num-1
                &.num-2
                    &:before
                        @extend .sprite-icon-tel-num-2
                &.num-3
                    &:before
                        @extend .sprite-icon-tel-num-3
    &_menu
        display flex
        flex 0 0 220px
        justify-content space-between
        align-items flex-end
        margin-left 100px
        @media screen and (max-width: 1200px)
            margin-left 10px
            flex 0 0 140px
        @media screen and (max-width: 767px)
            flex 0 0 auto
        &-link
            display flex
            flex-direction column
            justify-content flex-end
            align-items center
            font-size 10px
            text-decoration none
            color grayColor
            position relative
            font-family fontOpen
            @media screen and (max-width: 1200px)
                font-size 0
            @media screen and (max-width: 767px)
                &:not(.link-4)
                    display none
            &:before
                content ''
                position relative
                top 2px
                left 3px
            &.link-1
                &:hover,
                &.active
                    &:before
                        @extend .sprite-icon-top-menu-link-1-active    
                &:before
                    @extend .sprite-icon-top-menu-link-1
            &.link-2
                &:hover,
                &.active
                    &:before
                        @extend .sprite-icon-top-menu-link-2-active    
                &:before
                    @extend .sprite-icon-top-menu-link-2
            &.link-3
                &:hover,
                &.active
                    &:before
                        @extend .sprite-icon-top-menu-link-3-active    
                &:before
                    @extend .sprite-icon-top-menu-link-3
            &.link-4
                &:hover,
                &.active
                    &:before
                        @extend .sprite-icon-top-menu-link-4-active    
                &:before
                    @extend .sprite-icon-top-menu-link-4
        &-border
            background #e9c827
            height 16px
            flex 0 0 1px
            margin-bottom 16px
            position relative
            &:first-child
                display none
                @media screen and (max-width: 1200px)
                    display block
                @media screen and (max-width: 767px)
                    display none
            @media screen and (max-width: 1200px)
                top 8px
            @media screen and (max-width: 767px)
                display none
        &-cart-count
            position absolute
            right 0
            top 0
            width 16px
            height 16px
            display flex
            justify-content center
            align-items center
            background redColor
            color white
            border-radius 8px
            font-family fontReg
            font-size 10px
    &_smart-menu-button
        flex 0 0 55px
        height 55px
        background grayColor
        position relative
        left -10px
        z-index 2
        @media screen and (min-width: 767px)
            display none

.header-bottom
    background grayColor
    height 40px
    @media screen and (max-width: 767px)
        display none
    &-content
        display flex
        height 100%
    &_category-menu
        position relative
        flex 0 0 240px
        margin-right 16px
        @media screen and (max-width: 1200px)
            flex 0 0 180px
        &-button
            cursor pointer
            user-select none
            height 40px
            display flex
            align-items center
            padding-left 10px
            transition background 0.3s ease
            .header-bottom_category-menu.active &,
            .front &
                background defColor
            &_icon
                @extend .sprite-icon-section-menu-but
                margin-right 8px
            &_text
                text-transform uppercase
                &-1
                    font-size 12px
                    color placeholderColor
                &-2
                    color white
                    font-size 14px
                    font-family fontBold
        &-dropdown
            position absolute
            left 0
            top 40px
            z-index 2
            background white
            flex-direction column
            box-sizing border-box
            width 100%
            border 1px solid grayBorder
            border-top none
            animation-duration 0.3s
            display none
            .header-bottom_category-menu.active &,
            .front &
                display flex
            &_link
                text-decoration none
                color grayColor
                display flex
                flex 0 0 38px
                font-family fontLight
                align-items center
                padding-left 11px
                position relative
                &:not(:last-child)
                    border-bottom 1px solid grayBorder
                &:before
                    content ''
                    margin-right 15px
                &:after
                    content ''
                    @extend .sprite-icon-section-menu-arrow
                    position absolute
                    right 14px
                    top 50%
                    margin-top (-15px/2)
                &.link-1
                    padding-left 12px
                    &:before
                        @extend .sprite-icon-section-menu-link-1
                &.link-2
                    padding-left 12px
                    &:before
                        @extend .sprite-icon-section-menu-link-2
                        margin-right 14px
                &.link-3
                    &:before
                        @extend .sprite-icon-section-menu-link-3
                        margin-right 14px
                &.link-4
                    padding-left 13px
                    &:before
                        @extend .sprite-icon-section-menu-link-4
                &.link-5
                    padding-left 12px
                    &:before
                        @extend .sprite-icon-section-menu-link-5
                        margin-right 14px
                &.link-6
                    &:before
                        @extend .sprite-icon-section-menu-link-6
                        margin-right 13px
    &_sections-menu
        display flex
        flex-direction column
        height 100%
        justify-content center
        &_label
            text-transform uppercase
            font-size 12px
            color placeholderColor
            margin-bottom 2px
        &-links
            display flex
            &_link
                text-decoration none
                text-transform uppercase
                font-size 12px
                color white
                margin-right 12px
                position relative
                white-space nowrap
                @media screen and (max-width: 1200px)
                    margin-right 10px
                &:last-child
                    @media screen and (max-width: 1200px)
                        display none
                &-new
                    position absolute
                    width 30px
                    right -4px
                    top -17px
                    display flex
                    justify-content center
                    align-items center
                    height 14px
                    background redColor
                    font-size 9px
                    border-radius 2px
                    &:after
                        content ''
                        width 0
                        height 0
                        border-style solid
                        border-width 2px 2px 0 0
                        border-color redColor transparent transparent transparent
                        position absolute
                        right 2px
                        bottom -2px

.front-slider
    padding-bottom 23px
    border-bottom 2px solid yellowColor
    margin-bottom 18px
    @media screen and (max-width: 1200px)
        padding-bottom 20px
        margin-bottom 15px
    @media screen and (max-width: 767px)
        padding-bottom 10px
    &-content
        margin-left 240px
        @media screen and (max-width: 1200px)
            margin-left 180px
            position relative
        @media screen and (max-width: 767px)
            margin 0 -10px
        &-slider
            img
                display block
                width 960px
                height 400px
                @media screen and (max-width: 1200px)
                    width 100%
                    height 300px
                    object-fit cover
                @media screen and (max-width: 767px)
                    height 200px
        &-thumbs
            display flex
            @media screen and (max-width: 1200px)
                position absolute
                left 15px
                bottom 12px
                align-items center
            &_item
                flex 0 0 25%
                height 100px
                position relative
                transition box-shadow 0.2s ease
                @media screen and (max-width: 1200px)
                    flex 0 0 6px
                    height 6px
                    background white
                    margin-right 8px
                &.active
                    box-shadow 5px 5px 10px rgba(grayColor, 0.35)
                    z-index 2
                    @media screen and (max-width: 1200px)
                        box-shadow none
                        background yellowColor
                        flex 0 0 10px
                        height 10px
                img
                    display block
                    height 100%
                    width 100%
                    @media screen and (max-width: 1200px)
                        display none

.breadcrumbs
    display flex
    height 55px
    font-size 12px
    align-items center
    @media screen and (max-width: 767px)
        display none
    &_link
        text-decoration none
        color grayColor
        &:hover
            text-decoration underline
    &_current
        color redColor
    & + .blog-title
        @media screen and (max-width: 767px)
            display none
            & + .blog-created
                display none

.catalog-title
    display flex
    margin-bottom 25px
    justify-content space-between
    @media screen and (max-width: 767px)
        margin-bottom 0
    &_counts
        color placeholderColor
        @media screen and (max-width: 1200px)
            font-size 12px
        @media screen and (max-width: 767px)
            display none

.page-title
    font-weight normal
    text-transform uppercase
    font-size 16px
    color grayColor
    font-family fontBold
    margin-bottom 25px
    @media screen and (max-width: 767px)
        margin-bottom 0
        display flex
        height 45px
        align-items center
    .catalog-title &,
    .sections-top-title &,
    .wishes-title &,
    .cabinet-wishes-title &
        margin 0
    .search-title &
        margin-bottom 12px
                    
.catalog-content
    display flex
    &-sidebar
        order 1
        flex 0 0 240px
        max-width 240px
        @media screen and (max-width: 1200px)
            flex 0 0 180px
            max-width 180px
        @media screen and (max-width: 767px)
            display none
        &-filters
            background white
            border 1px solid grayBorder
            padding 20px 0
            color grayColor
            &_panel
                padding-left 20px
                padding-right 18px
                @media screen and (max-width: 1200px)
                    padding-left 10px
                    padding-right 10px
                &:not(:last-child)
                    padding-bottom 24px
                    border-bottom 1px solid grayBorder
                    margin-bottom 20px
            &_title
                cursor pointer
                user-select none
                display flex
                justify-content space-between
                align-items center
                font-family fontBold
                font-size 16px
                &:after
                    content ''
                    width 0
                    height 0
                    border-style solid
                    border-width 0 4px 4px 4px
                    border-color transparent transparent yellowColor transparent
                    transition transform 0.3s ease
                    .catalog-content-sidebar-filters_panel.closed &
                        transform rotate(180deg)
            &-list
                display flex
                flex-direction column
                padding-top 17px
                transition-duration 0.2s
                .sections-filters &
                    padding-left 10px
                .catalog-content-sidebar-filters_panel.closed &
                    display none
                &_link
                    display flex
                    align-items center
                    text-decoration none
                    color grayColor
                    @media screen and (max-width: 1200px)
                        font-size 12px
                    .sections-filters &.active,
                    .sections-filters &:hover
                        color redColor
                    &:not(:last-child)
                        margin-bottom 14px
                    &_checkbox
                        box-sizing border-box
                        flex 0 0 16px
                        height 16px
                        border 1px solid grayBorder
                        border-radius 2px
                        margin-right 8px
                        position relative
                        &:after
                            content ''
                            @extend .sprite-icon-checkbox
                            position absolute
                            left 1px
                            top 0
                            opacity 0
                            visibility hidden
                            transition all 0.2s ease
                            .catalog-content-sidebar-filters-list_link.active &,
                            .catalog-filter-small-dropdown-sort_link.active &
                                opacity 1
                                visibility visible
                    &_counts
                        color placeholderColor
                        margin-left 4px
            &-price
                display flex
                align-items center
                &_text
                    height 24px
                    border 1px solid grayBorder
                    text-align center
                    border-radius 2px
                    width 70px
                    font-size 14px
                    color grayColor
                    @media screen and (max-width: 1200px)
                        width 60px
                        font-size 12px
                &_border
                    flex 0 0 10px
                    height 1px
                    background grayBorder
                    @media screen and (max-width: 1200px)
                        flex 0 0 5px
                &_action
                    cursor pointer
                    margin-left 9px
                    height 24px
                    width 40px
                    text-transform uppercase
                    color grayColor
                    background yellowColor
                    border none
                    @media screen and (max-width: 1200px)
                        margin-left 5px
                        width 30px
    &-main
        order 2
        flex 1 0 0

.product-top-filters
    display flex
    height 30px
    justify-content space-between
    padding-left 20px
    font-size 12px
    margin-bottom 10px
    color placeholderColor
    @media screen and (max-width: 1200px)
        padding-left 0
    @media screen and (max-width: 767px)
        display none
    &-list
        display flex
        flex 1 0 0
        &-views
            display flex
            align-items center
            flex 0 0 130px
            @media screen and (max-width: 1200px)
                flex 0 0 75px
            &_label
                margin-right 12px
                @media screen and (max-width: 1200px)
                    display none
            &_link
                box-sizing border-box
                display flex
                justify-content center
                align-items center
                flex 0 0 30px
                height 30px
                background white
                &.active
                    border 1px solid grayBorder
                    &.view-1
                        &:after
                            @extend .sprite-icon-product-view-1-active
                    &.view-2
                        &:after
                            @extend .sprite-icon-product-view-2-active
                &:after
                    content ''
                &.view-1
                    margin-right 5px
                    &:after
                        @extend .sprite-icon-product-view-1
                &.view-2
                    &:after
                        @extend .sprite-icon-product-view-2
        &-sort
            display flex
            align-items center
            flex 0 0 248px
            @media screen and (max-width: 1200px)
                flex 0 0 150px
            &_label
                margin-right 8px
                white-space nowrap
                @media screen and (max-width: 1200px)
                    display none
            &_select
                position relative
                flex 0 0 140px
                height 30px
                .chosen-container
                    top 0
                    left 0
                .chosen-single
                    position relative
                    &:after
                        content ''
                        width 0
                        height 0
                        border-style solid
                        border-width 0 4px 4px 4px
                        border-color transparent transparent redColor transparent
                        position absolute
                        top 8px
                        right 10px
                    div
                        position relative
                        top 4px
                .chosen-results li
                    justify-content space-between
                    padding-right 10px
                    &:after
                        content ''
                        width 0
                        height 0
                        border-style solid
                    &.up
                        &:after
                            border-width 0 4px 4px 4px
                            border-color transparent transparent redColor transparent
                    &.down
                        &:after
                            border-width 4px 4px 0 4px
                            border-color redColor transparent transparent transparent                            
        &-compare
            text-decoration none
            display flex
            box-sizing border-box
            align-items center
            height 30px
            flex 0 0 140px
            background white
            border 1px solid redColor
            color grayColor
            justify-content space-between
            padding 0 6px 0 13px
            &_count
                display flex
                justify-content center
                align-items center
